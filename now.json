{
  "version": 2,
  "name": "dolar-uvita-frontend",
  "builds": [{ "src": "next.config.js", "use": "@now/next" }],
  "routes": [
    {
      "src": "/_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": { "cache-control": "immutable" }
    }
  ],
  "env": {
    "APP_PATH": "@app_path"
  }
}
